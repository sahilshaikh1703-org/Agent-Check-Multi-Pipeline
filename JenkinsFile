pipeline{
    
    tools{
        maven 'MVN_HOME'
    }

    agent any
    
   stages{
       stage('Clone a Repo'){
           steps{
              git branch: 'dev', url: 'https://github.com/sahilshaikh1703-org/multiPipelineRepo.git'
           }
       }
       
       stage('Compile the code'){
           steps{
              sh 'mvn compile'
           }
       }
       
       
       stage('Unit Test'){
           steps{
              sh 'mvn test'
           }
       }
       
     
       }
}
